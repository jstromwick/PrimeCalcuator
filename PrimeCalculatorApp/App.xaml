<Application x:Class="PrimeCalculatorApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:PrimeCalculatorApp.ViewModels"
             xmlns:converters="clr-namespace:PrimeCalculatorApp.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <viewModels:CalculatorViewModel x:Key="CalculatorViewModel" />
        <BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter"></BooleanToVisibilityConverter>
        <converters:InvertBoolConverter x:Key="InvertBoolConverter"></converters:InvertBoolConverter>
    </Application.Resources>
</Application>



